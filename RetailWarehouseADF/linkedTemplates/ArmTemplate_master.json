{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "RetailWarehouseADF"
		},
		"AzureDataLakeStaging_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStaging'"
		},
		"AzureSqlRetailDataWarehouse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlRetailDataWarehouse'"
		},
		"OnPremSqlServerDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPremSqlServerDatabase'"
		},
		"AzureDataLakeStaging_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://retailstaging.dfs.core.windows.net"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://OnPremDevices.vault.azure.net/"
		},
		"FlatFileLocalPc_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\LAPTOP-SVMTPL4C\\AzureLoadFlatSource"
		},
		"FlatFileLocalPc_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "grmanojbabu8@gmail.com"
		},
		"OnPremSqlServerDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "grmanojbabu8@gmail.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "RetailWarehouseADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStaging_accountKey": {
						"value": "[parameters('AzureDataLakeStaging_accountKey')]"
					},
					"AzureSqlRetailDataWarehouse_connectionString": {
						"value": "[parameters('AzureSqlRetailDataWarehouse_connectionString')]"
					},
					"OnPremSqlServerDatabase_connectionString": {
						"value": "[parameters('OnPremSqlServerDatabase_connectionString')]"
					},
					"AzureDataLakeStaging_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStaging_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"FlatFileLocalPc_properties_typeProperties_host": {
						"value": "[parameters('FlatFileLocalPc_properties_typeProperties_host')]"
					},
					"FlatFileLocalPc_properties_typeProperties_userId": {
						"value": "[parameters('FlatFileLocalPc_properties_typeProperties_userId')]"
					},
					"OnPremSqlServerDatabase_properties_typeProperties_userName": {
						"value": "[parameters('OnPremSqlServerDatabase_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}